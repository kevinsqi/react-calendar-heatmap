{
  "name": "react-calendar-heatmap",
  "version": "1.6.1",
  "description": "A calendar heatmap component built on SVG, inspired by github's commit calendar graph.",
  "author": "PatientsLikeMe Engineering",
  "main": "./build/index.js",
  "types": "./types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/patientslikeme/react-calendar-heatmap.git"
  },
  "license": "MIT",
  "keywords": [
    "calendar",
    "react",
    "react-component",
    "svg"
  ],
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "build:demo": "NODE_ENV=demo webpack",
    "clean": "rimraf build",
    "deploy:demo": "npm run build:demo && ./scripts/build_ghpages.sh",
    "lint": "eslint --ext .js,.jsx --cache . && echo \"eslint: no lint errors found\" || true",
    "prepublish": "npm run clean && npm run build",
    "start": "webpack-dev-server --progress --inline",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "types": "tsc -p ./types/test/tsconfig.json"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "@types/react": "^16.0.5",
    "babel-core": "^6.3.15",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.8.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.2",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.3.4",
    "typescript": "^2.5.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.0.0"
  }
}
